package Assignment1;

public class Apartmenter {

        private int houses;
        private char max_size;
        private char floors;
        private int people;
        private int security;
        public void people(int p,int s){
            people=p;
            security=s;
        }
        public void total(){
            System.out.println("the apartment has "+people+" peoples and "+security+" securities in it.");
        }
        public char getMax_size() {

            System.out.println("apartment size is "+max_size+" cents");
            return 0;
        }

        public char getFloors() {
            System.out.println("No of floors in the apartment is "+floors);
            return 0;
        }

        public void setFloors(char floors) {
            this.floors = floors;
        }

        public void setMax_size(char max_size) {
            this.max_size = max_size;
        }

        public char getHouses() {
            System.out.println("This apartment consists of "+houses+" houses in total");
            return 0;
        }

        public void setHouses(int houses) {
            this.houses = houses;
        }

    }



